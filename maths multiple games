print("Math Game!")
multiple= input("Name your multiples:")
count=0
for x in range(1,11):
 answer = x* multiple
 print(x,"*",multiple)
 ans= int(input("="))
 if ans == answer:
   print("you got the right answer.")
 else:
   print("not a correct answer. Correct Answer is", answer)
if count ==10:
   print("Wow! You got all correct.")
else: 
  print("you got",count,"out of 10.")
